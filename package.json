{
  "name": "wikibox",
  "version": "1.0.0",
  "description": "Modern and Simple CMS works on the browser, which inspired by MDWiki",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx --fix ./src",
    "format": "prettier --write ./*.{js,ts,tsx} ./src/*.{js,ts,tsx}",
    "clean": "rimraf dist/ src/**/*~",
    "build": "gulp",
    "serve": "npx http-server dist/",
    "inspect": "npx source-map-explorer dist/default/lib/index.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bibliobibulus/wikibox.git"
  },
  "keywords": [
    "markdown",
    "wiki"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/bibliobibulus/wikibox/issues"
  },
  "homepage": "https://github.com/bibliobibulus/wikibox#readme",
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.0",
    "bootswatch": "^5.0.0",
    "highlight.js": "^10.7.2",
    "lodash.template": "^4.5.0",
    "rehype-highlight": "^4.1.0",
    "rehype-mathjax": "^3.1.0",
    "rehype-react": "^6.2.0",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "rehype-toc": "^3.0.2",
    "remark-directive": "^1.0.1",
    "remark-math": "^4.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "esbuild": "^0.11.19",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-esbuild": "^0.7.6",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-zip": "^5.1.0",
    "http-server": "^0.12.3",
    "merge-stream": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2"
  }
}
