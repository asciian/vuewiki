{
  "name": "wikibox",
  "version": "0.2.0",
  "description": "Modern and Simple CMS works on the browser, which inspired by MDWiki",
  "scripts": {
    "lint": "eslint --fix ./src/*",
    "format": "prettier --write ./*.{js,ts,tsx} ./src/*.{js,ts,tsx}",
    "clean": "rm -rf build/",
    "build": "NODE_ENV=production parallel-webpack",
    "postbuild": "find build -name '*.js' -exec rm {} \\;",
    "build:dev": "NODE_ENV=development parallel-webpack",
    "postbuild:dev": "find build -name '*.js' -exec rm {} \\;",
    "start": "NODE_ENV=development webpack-dev-server --hot --port=6789"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta2gch/wikibox.git"
  },
  "keywords": [
    "markdown",
    "wiki"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/ta2gch/wikibox/issues"
  },
  "homepage": "https://github.com/ta2gch/wikibox#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "cheerio": "^1.0.0-rc.3",
    "code-elements": "fgborges/code-elements",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fp-ts": "^1.15.0",
    "latex-elements": "fgborges/latex-elements",
    "react": "^16.8.5",
    "react-bootstrap": "^1.0.0-beta.6",
    "react-dom": "^16.8.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.2",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/cheerio": "^0.22.12",
    "@types/node": "^10.14.4",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-hash-link": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "3",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "parallel-webpack": "^2.3.0",
    "prettier": "^1.16.4",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
