{
  "name": "wikibox",
  "version": "0.2.0",
  "description": "Modern and Simple CMS works on the browser, which inspired by MDWiki",
  "scripts": {
    "lint": "tslint --fix ./src/*",
    "format": "prettier --write ./*.{js,ts,tsx} ./src/*.{js,ts,tsx}",
    "clean": "rm -rf build/",
    "build": "NODE_ENV=production parallel-webpack && find build -name '*.js' -exec rm {} \\;",
    "start": "NODE_ENV=development webpack-dev-server --hot --port=6789"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta2gch/wikibox.git"
  },
