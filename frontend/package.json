{
  "name": "wikibox",
  "version": "0.2.0",
  "description": "Modern and Simple CMS works on the browser, which inspired by MDWiki",
  "scripts": {
    "lint": "tslint --fix ./src/*",
    "format": "prettier --write ./src/*",
    "prestart": "pcregrep -o -M '```typescript[\\s\\S]*?```' ../data/api.md | grep -v '```' > src/api.ts",
    "prebuild": "pcregrep -o -M '```typescript[\\s\\S]*?```' ../data/api.md | grep -v '```' > src/api.ts",
    "build": "NODE_ENV=production webpack --progress",
    "start": "NODE_ENV=development THEME=journal webpack-dev-server --hot --port=6789"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtaniguchimasaya/wikibox.git"
  },
  "keywords": [
    "markdown",
    "wiki"
  ],
  "author": "TANIGUCHI Masaya",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/asciian/WikiBox/issues"
  },
  "homepage": "https://github.com/asciian/WikiBox#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@types/react-fontawesome": "^1.6.4",
    "bootswatch": "^4.1.1",
    "es6-promisify": "^6.0.0",
    "fp-ts": "^1.8.0",
    "katex": "^0.10.0-rc.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-katex": "^2.0.2",
    "react-markdown": "^3.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "reactstrap": "^6.4.0",
    "remark": "^9.0.0",
    "remark-html": "^8.0.0",
    "remark-math": "^1.0.4",
    "slugify": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/es6-promisify": "^6.0.0",
    "@types/highlight.js": "^9.12.3",
    "@types/localforage": "0.0.34",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-scroll": "^1.5.4",
    "@types/reactstrap": "^6.0.2",
    "@types/remarkable": "^1.7.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "prettier": "^1.14.2",
    "tslint": "^5.11.0",
